package ejercicio1;
import java.util.Scanner;

public class Ejercicio1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		final int local=5;
		final int visitante=5;
		int golLocal;
		
		int [][] partidos = new int [local][visitante];
		int[] golesLocal = new int [local];
		for (int i=0; i<golesLocal; i++) {
			golLocal= (int)(Math.random()*5);
		}
	
		for(int i=0; i<local ; i++) {
			partidos [i] = golesLocal;
			System.out.println(golesLocal[i]);
		}
		
			
		
		
	}

}
