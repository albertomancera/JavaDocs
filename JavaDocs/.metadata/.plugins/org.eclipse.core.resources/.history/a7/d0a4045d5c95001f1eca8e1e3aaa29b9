 */
package ejercicio9;
import java.util.*;

public class Ejercicio9 {
	public static void main(String args[]) {
		Scanner entradaTeclado = new Scanner(System.in);
		
		int array[]= new int[100];
		for (int i=0; i<100;i++) {
			array[i]=(int) (1+Math.random()*10);
			
		System.out.println("Dame un valor entero N (entre 1 y 10)");
		int n= entradaTeclado.nextInt();
		
		if(n==array[i]) {
			System.out.println("");
		}
		}
	}
}
