package ejemplonumprimos;
import java.util.Scanner;

public class NumerosPrimos {
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);

		int num=17;
		
		
	
		if(num/2==0) {
			System.out.println("Este número no es primo");
		}else if (num/3==0) {
			System.out.println("Este número no es primo");
		}
		

	}
	}

