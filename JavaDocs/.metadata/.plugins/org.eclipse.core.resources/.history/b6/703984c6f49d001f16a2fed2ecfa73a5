		System.out.println("Dime n√∫meros enteros: ");
		valor = entradaTeclado.nextInt();
		
		while(valor !=0) {
			if (resto == devolverresto(valor)) {
				
			}
		}
		
	
		
	}
	//Comprobar si es multiplo de 5
	public static int devolverresto(int num) {
		return num%5;
	}
}
