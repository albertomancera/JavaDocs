/*
 * Objetivo: Almacenar y mostrar datos de un array y sumar los valores
 * Autor: Alberto Mancera Plaza
 * Fecha: 28/10/2024
 */
package ejercicio2;
import java.util.*;

public class Ejercicio2 {
	public static void main(String args[]) {
		Scanner entradaTeclado = new Scanner(System.in);
	
		int suma=0;
		
		int[]num = new int[10]; //Array de 10 numeros
		for (int i=0; i<10; i++) {
			System.out.println("Dime un número real: ");
			num[i]= entradaTeclado.nextInt();
			suma +=num[i];
		}
		//para que me muestre los datos
		for (int i=0; i<10; i++) {
			System.out.println("La suma de estos número es: "+suma");
		}
	}
}
