		nombre = entradaTeclado.nextLine();
		
		String apellidos;
		System.out.println("Dime tu apellido");
		apellidos = entradaTeclado.nextLine();
		
		String fechanacimiento;
		System.out.println("Dime tu fecha de nacimiento");
		fechanacimiento = entradaTeclado.nextLine();
		
		double salariobruto;
		System.out.println("Dime tu salario bruto");
		salariobruto = entradaTeclado.nextLine();
		
		int annos;
		System.out.println("Dime tus a√±os trabajando en la empresa");
		annos = entradaTeclado.nextLine();
		
	}
}
