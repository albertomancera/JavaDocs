package ejercicio2;

public class Ejercicio2 {
	public static void main(String args[]) {
		
		int columnas=10;
		int filas=10;
		
		int tabla[][]=new int [columnas][filas];
		for (int i=0;i<columnas;i++) {
			for(int j=0;j<filas;j++) {
				tabla[i][j]=(i+1)*(j+1);
			}
		}
		for (int i=0;i< columnas ;i++) {
			for(int j=0;j<filas;j++) {
			System.out.print(tabla[i][j]);
			}
			System.out.printf();
		}
	}
}
