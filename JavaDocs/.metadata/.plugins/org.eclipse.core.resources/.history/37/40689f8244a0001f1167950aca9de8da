		Scanner sc  = new Scanner (System.in);
		double a, b;
		a = 10;
		b=25;
		String operador="";
		
		
		
	}
		
		public static double operacion (double a, double b, String operador) {
			double resultado;
			switch (operador) {
			case "+":
				a+b;
			break;
			case "-":
				a-b;
			break;
			case "*":
				a*b
			
			
				
				
			}
