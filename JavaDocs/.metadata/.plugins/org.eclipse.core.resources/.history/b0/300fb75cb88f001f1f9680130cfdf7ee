package array;

public class Array {
	public static void main(String args[]) {
		
		int [] num = new int [10];
		
		for (int i=0; i < num.length; i++) {
			num [i] = (int)  ( 25* 2% Math.random()) ;
			System.out.println("El contenido de la posición: "+ i +" es " +num[i]);
		}
		
		/*for (int i=0; i < num.length; i++) {
			System.out.println("El contenido de la posición: "+ i +" es " +num[i]);
		}*/
		
	}
}
