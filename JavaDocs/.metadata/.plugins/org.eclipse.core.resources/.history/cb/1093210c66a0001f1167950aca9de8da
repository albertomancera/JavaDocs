		System.out.println("Dime un n√∫mero: ");
		num1 = sc.nextInt();
		if (minimo(num1,num2)) {
			
		}
	}
	public static int minimo (int num1,int num2) {
		int min;
		
		if (num1<num2) {
			min = num1;
		}else if (num2<num1) {
			min = num2;
		}else {
			min = 0;
		}
		return min;
	}
}
