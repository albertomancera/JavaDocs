package poo;

public class Principal {
	public static void main (String[] args) {
	try {
		Persona p = new Persona ("Fernando", "Lopez", 25);
		
		System.out.println(p.getNombre);
	}catch (IllegalArgumentException e) {
		System.out.println(e.getMessage());
	}
	
	System.out.println(p.getNombre);
	
}
}
