package ejercicio2;
import java.util.*;

public class Ejercicio2 {
	public static void main(String[] args) {
		Scanner entradaTeclado = new Scanner(System.in);
		
		
		String caracter1 ,caracter2, n=n, o=o;
		
		do {
		System.out.println("Dime un caracter: ");
		caracter1=entradaTeclado.nextLine();
		
		System.out.println("Dime otro caracter: ");
		caracter2=entradaTeclado.nextLine();
		
		if(caracter1.equals(caracter2)) {
			System.out.println("Los caracteres son iguales");
		}else {
			if(caracter1!=caracter2) {
				System.out.println("Los caracteres son diferentes");
			}else {
				System.out.println("El caracter introducido es erroneo");		
		}		
		}	
		}while(caracter1==n,)
		
		
		

		
	}
}
